public class Main {
	public static class Source {
		static void execute() {
			public class MyClass {
				
				/** CLASS_INFO = """This is my class""" */;
				double a = 10;
				char b = "4";
				String c = "Panda ";
				c = c + b ;
				boolean d = true;
				
				public void func() {
				
					if ((d)){
					
						System.out.println("Hello");
						
					
					}
					
				
				}
				
			}
			
			System.out.println(MyClass.a);
			System.out.println(MyClass.func);
			System.out.println("This is my class");
			
		}
	}
	
	public static void main(String[] args) {
		Source.execute();
	}
}